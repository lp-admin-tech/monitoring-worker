# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# AI Model Configuration (choose one provider)
# For Alibaba Tongyi DeepSeek (free tier)
AI_MODEL_API_KEY=your_alibaba_api_key_here
AI_MODEL_PROVIDER=alibaba
AI_MODEL_NAME=alibaba/tongyi-qwen-plus

# Alternative: OpenRouter Configuration (free Tongyi DeepResearch 30B)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=alibaba/tongyi-deepresearch-30b-a3b:free

# Google Ad Manager (GAM) Configuration
GAM_API_KEY=your_gam_api_key_here
GAM_ACCOUNT_ID=your_gam_account_id_here

# Worker Configuration
WORKER_SECRET=your-worker-secret-here
PORT=9001
NODE_ENV=production
LOG_LEVEL=INFO

# Playwright Configuration (for Render.com deployment)
PLAYWRIGHT_BROWSERS_PATH=/opt/render/.cache/ms-playwright

# Optional: Google Safe Browsing API (https://developers.google.com/safe-browsing/v4/get-started)
# Leave empty to skip safe browsing checks
GOOGLE_SAFE_BROWSING_API_KEY=
